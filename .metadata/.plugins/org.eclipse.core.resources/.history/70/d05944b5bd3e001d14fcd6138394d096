import java.util.Random;
import java.util.Scanner;

public class Exercicio6 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		Random generator = new Random();
		
		int linhaA, linhaB;
		int colunaA, colunaB;
		int soma;
		
		System.out.println("Total de Linhas da Matriz A -->");
		linhaA=sc.nextInt();
		System.out.println("Total de Colunas da Matriz A -->");
		colunaA=sc.nextInt();
		linhaB=colunaA;
		System.out.println("Total de Colunas da Matriz B -->");
		colunaB=sc.nextInt();
		
		int arr1 [][] = new int[linhaA][colunaA];
		int arr2 [][] = new int[linhaB][colunaB];
		int arr3 [][] = new int[linhaA][colunaB];
		
		System.out.println("Matriz A: ");
		for(int i=0; i<arr1.length; i++) {
			for(int j=0; j<arr1[i].length; j++) {
				arr1 [i][j] = generator.nextInt(10);
				System.out.print(arr1[i][j] +" ");
			}
			System.out.println();
		}
		
		System.out.println("\nMatriz B");
		
		
		for(int i=0; i<arr2.length; i++) {
			for(int j=0; j<arr2[i].length; j++) {
				arr2 [i][j] = generator.nextInt(10);
				System.out.print(arr2[i][j] +" ");
			}
			System.out.println();
		}
		
		//multiplicao de A e B gerando C
		for(int i=0; i<linhaA;i++) {
			for(int k=0; k<colunaB;k++) {
				soma = 0;
				for(int j=0; j<colunaA; j++) {
					soma += a[i][j] * b[j][k];
					arr3[i][k]=soma;
				}
			}
		}
		
		System.out.println("\nMatriz C: ");
		
		for(int i=0; i<arr3.length; i++) {
			for(int j=0; j<arr3[i].length; j++) {
				System.out.print(arr3[i][j] +" ");
			}
			System.out.println();
		}
		
		
		
	}

}
